{
  "manifest_version": 3,
  "name": "CourseGrabber",
  "version": "1.0",
  "description": "Helps students grab courses",
  "action": {
    "default_popup": "CourseGrabber.html",
    "default_icon": "CourseGrabber.png"
  },
  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "*://acadinfo.wustl.edu/*",
    "https://acadinfo.wustl.edu/apps/Registration/"
  ],
  "background": {
    "service_worker": "background.js"
  }

}

