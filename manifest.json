{
  "manifest_version": 3,
  "name": "CourseGrabber",
  "version": "1.0",
  "description": "Helps students grab courses",
  "action": {
    "default_popup": "CourseGrabber.html",
    "default_icon": "CourseGrab.png"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "https://courses.wustl.edu/*",
        "https://acadinfo.wustl.edu/*"
      ],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ]
}
